<?xml version="1.0" encoding="utf-8" ?>
<Mapping>
  <!-- Map specific named files to an alternative folder -->
  <FileToPath map="PluginAssemblies\**\*.*" to="..\..\MyPlugins\bin\**" />
  <!-- Map any file in and under WebResources to an alternate set of sub-folders -->
  <FileToPath map="WebResources\*.*" to="..\..\Webresources\**" />
  <FileToPath map="WebResources\**\*.*" to="..\..\Webresources\**" />
</Mapping>